<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <add value="index.asp" />
            </files>
        </defaultDocument>
        <httpErrors errorMode="Detailed" />
        <staticContent>
            <remove fileExtension=".*" />
            <mimeMap fileExtension=".*" mimeType="*/*" />
        </staticContent>
    </system.webServer>
</configuration>
